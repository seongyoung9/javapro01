<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.mysql.mapperPhone">
    
    <select id="getPhoneList" resultType="ModelPhone">  <!-- select * from tb_phone; -->
        SELECT *
          FROM tb_phone
    </select>   
        
    <insert id="insertPhone" parameterType="ModelPhone"> <!--  insert into tb_phone(name, manufacturer, price) values('aaa','bbb',100) ; -->
        INSERT INTO 
        tb_phone   (  name,   manufacturer,   price)
        values     (#{name},#{manufacturer},#{price})
    </insert>
    
 
 </mapper>
